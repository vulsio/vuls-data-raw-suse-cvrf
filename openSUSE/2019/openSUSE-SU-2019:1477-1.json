	"document_notes": [
		{
			"text": "Security update for gnutls",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for gnutls fixes the following issues:\n\nSecurity issue fixed:\n\n- CVE-2018-16868: Fixed Bleichenbacher-like side channel leakage in PKCS#1 v1.5 verification (bsc#1118087).\n\nNon-security issue fixed:\n\n- Explicitly require libnettle 3.4.1 to prevent missing symbol errors (bsc#1134856).\n\nThis update was imported from the SUSE:SLE-15:Update update project.",
			"title": "Details",
			"type": "General"
		},
