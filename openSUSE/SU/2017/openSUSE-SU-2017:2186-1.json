	"document_notes": [
		{
			"text": "Security update for openjpeg2",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for openjpeg2 fixes the following issues:\n\n- CVE 2016-7163: Integer Overflow could lead to remote code execution (bsc#997857)\n- CVE 2015-8871: Use-after-free in opj_j2k_write_mco function could lead to denial of service (bsc#979907)\n\nThis update was imported from the SUSE:SLE-12-SP2:Update update project.",
			"title": "Details",
			"type": "General"
