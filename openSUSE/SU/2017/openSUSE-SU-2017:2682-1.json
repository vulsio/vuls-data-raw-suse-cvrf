	"document_notes": [
		{
			"text": "Security update for mpg123",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for mpg123 to version 1.25.7 fixes the following issues:\n\n- CVE-2017-10683: Improvement over previous fix for xrpnt overflow problems (boo#1046766)\n\nThe following changes are also included in version 1.25.7:\n\n- Do not play with cursor and inverse video for progress bar when TERM=dumb\n- Fix parsing of host port for numerical IPv6 addresses\n",
			"title": "Details",
			"type": "General"
		},
