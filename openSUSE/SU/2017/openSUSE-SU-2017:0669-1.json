	"document_notes": [
		{
			"text": "Security update for bitlbee",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "\nThis update for bitlbee fixes the following security issues:\n\n- A file transfer request from a contact not in the contact list could have\n  resulted in a null pointer dereference, causing remote DoS by malicious\n  remote clients (CVE-2016-10189, bnc#1022498).\n",
			"title": "Details",
			"type": "General"
		},
