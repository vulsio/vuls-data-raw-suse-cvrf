	"document_notes": [
		{
			"text": "Security update for lynx",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "\nThis update for lynx fixes the following issues:\n\n- CVE-2016-9179: It was found that Lynx doesn't parse the authority\n  component of the URL correctly when the host name part ends with '?',\n  and could instead be tricked into connecting to a different host.\n  (bsc#1008642)\n\nThis update was imported from the SUSE:SLE-12:Update update project.",
			"title": "Details",
			"type": "General"
