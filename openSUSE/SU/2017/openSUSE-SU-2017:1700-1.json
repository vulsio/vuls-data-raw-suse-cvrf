	"document_notes": [
		{
			"text": "Security update for libgcrypt",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for libgcrypt fixes the following issues:\n\n- CVE-2017-9526: Store the session key in secure memory to ensure that constant\n  time point operations are used in the MPI library.  (bsc#1042326)\n\n- Don't require secure memory for the fips selftests, this prevents the\n  'Oops, secure memory pool already initialized' warning. (bsc#931932)\n\nThis update was imported from the SUSE:SLE-12:Update update project.",
			"title": "Details",
			"type": "General"
		},
