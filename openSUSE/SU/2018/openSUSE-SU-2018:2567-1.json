	"document_notes": [
		{
			"text": "Security update for libX11",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for libX11 fixes the following issues:\n\nSecurity issues fixed:\n\n- CVE-2018-14598: Fixed a crash on invalid reply in XListExtensions (boo#1102073)\n- CVE-2018-14599: Fixed an off-by-one write in XListExtensions (boo#1102062)\n- CVE-2018-14600: Fixed an out of boundary write in XListExtensions (boo#1102068)\n",
			"title": "Details",
			"type": "General"
		},
