	"document_notes": [
		{
			"text": "Security update for bsh2",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "\nThis update for bsh2 fixes the following issues: \n\n- CVE-2016-2510: An application that includes BeanShell on the\n  classpath may be vulnerable if another part of the application uses Java\n  serialization or XStream to deserialize data from an untrusted source.\n\nPlease see https://github.com/beanshell/beanshell/releases/tag/2.0b6 for more information.\n\nThis update was imported from the SUSE:SLE-12:Update update project.",
			"title": "Details",
			"type": "General"
		},
