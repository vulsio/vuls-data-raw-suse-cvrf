	"document_notes": [
		{
			"text": "Security update for MozillaFirefox",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "\nThis update to MozillaFirefox 43.0.3 fixes the following issues:\n\n* CVE-2015-7575: MD5 signatures accepted within TLS 1.2 ServerKeyExchange in server signature, in combination wit NSS 3.20.2 [boo#959888]\n\nFurther fixes:\n\n* workaround Youtube user agent detection issue (bmo#1233970)\n* fix file download regression for multi user systems\n",
			"title": "Details",
			"type": "General"
		},
