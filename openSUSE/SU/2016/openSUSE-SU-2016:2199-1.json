	"document_notes": [
		{
			"text": "Security update for libqt4",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "\nThis update for libqt4 fixes the following issues:\n\nVarious unsafe SSL ciphers have been disabled in the standard SSL classes.\nAlso the RC4 based ciphers have been disabled. (bsc#865241)\n\nThis update was imported from the SUSE:SLE-12:Update update project.",
			"title": "Details",
			"type": "General"
