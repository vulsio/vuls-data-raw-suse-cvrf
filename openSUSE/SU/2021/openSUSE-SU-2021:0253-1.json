	"document_notes": [
		{
			"text": "Security update for cups",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for cups fixes the following issues:\n\n- CVE-2020-10001: Fixed an out-of-bounds read in the ippReadIO function (bsc#1180520).\n- CVE-2019-8842: Fixed an out-of-bounds read in an extension field (bsc#1170671).\n\nThis update was imported from the SUSE:SLE-15:Update update project.",
			"title": "Details",
			"type": "General"
