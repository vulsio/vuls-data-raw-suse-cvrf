	"document_notes": [
		{
			"text": "Security update for qemu",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for qemu fixes the following issues:\n\n- CVE-2021-3546: Fixed out-of-bounds write in virgl_cmd_get_capset (bsc#1185981).\n- CVE-2021-3544: Fixed memory leaks found in the virtio vhost-user GPU device (bsc#1186010).\n- CVE-2021-3545: Fixed information disclosure due to uninitialized memory read (bsc#1185990).\n",
			"title": "Details",
			"type": "General"
		},
