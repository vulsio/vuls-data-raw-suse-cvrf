	"document_notes": [
		{
			"text": "Security update for nodejs14",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for nodejs14 fixes the following issues:\n\n- CVE-2021-3672: Fixed missing input validation on hostnames (bsc#1188881).\n- CVE-2021-22931: Fixed improper handling of untypical characters in domain names (bsc#1189370).\n- CVE-2021-22940: Use after free on close http2 on stream canceling (bsc#1189368)\n- CVE-2021-22939: Incomplete validation of rejectUnauthorized parameter (bsc#1189369)\n- CVE-2021-22930: Fixed use after free on close http2 on stream canceling (bsc#1188917). \n",
			"title": "Details",
			"type": "General"
		},
