	"vulnerability": [
		{
			"notes": [
				{
					"text": "An integer overflow could occur when OpenEXR processes a crafted file on systems where size_t < 64 bits. This could cause an invalid bytesPerLine and maxBytesPerLine value, which could lead to problems with application stability or lead to other attack paths.",
					"title": "Vulnerability Description",
					"type": "General"
				}
			],
