	"document_notes": [
		{
			"text": "Security update for lighttpd",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for lighttpd to version 1.4.54 fixes the following issues:\n\nSecurity issues fixed:\n\n- CVE-2018-19052: Fixed a path traversal in mod_alias (boo#1115016).\n- Changed the default TLS configuration of lighttpd for better security out-of-the-box (boo#1087369).\n",
			"title": "Details",
			"type": "General"
		},
