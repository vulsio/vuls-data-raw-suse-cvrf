	"vulnerability": [
		{
			"notes": [
				{
					"text": "Cross-site scripting (XSS) vulnerability in the print_fileinfo function in ui-diff.c in cgit 0.9.0.2 and earlier allows remote authenticated users to inject arbitrary web script or HTML via the filename associated with the rename hint.",
					"title": "Vulnerability Description",
					"type": "General"
				}
			],
			"cve": "CVE-2011-2711",
