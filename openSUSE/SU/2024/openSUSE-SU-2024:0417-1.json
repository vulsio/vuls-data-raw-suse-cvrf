	"document_notes": [
		{
			"text": "Security update for chromium",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "\nThis security update for Chromium to version 131.0.6778.204 (boo#1234704) fixes:\n\n* CVE-2024-12692: Type Confusion in V8\n* CVE-2024-12693: Out of bounds memory access in V8\n* CVE-2024-12694: Use after free in Compositing\n* CVE-2024-12695: Out of bounds write in V8\n* Various fixes from internal audits, fuzzing and other initiatives\n",
			"title": "Details",
			"type": "General"
		},
