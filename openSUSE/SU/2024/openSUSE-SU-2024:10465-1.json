	"vulnerability": [
		{
			"notes": [
				{
					"text": "The mipv6 daemon in UMIP 0.4 does not verify that netlink messages originated in the kernel, which allows local users to spoof netlink socket communication via a crafted unicast message.",
					"title": "Vulnerability Description",
					"type": "General"
				}
			],
