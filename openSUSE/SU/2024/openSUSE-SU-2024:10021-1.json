	"vulnerability": [
		{
			"notes": [
				{
					"text": "The process_stat function in Memcached 1.2.8 discloses memory-allocation statistics in response to a stats malloc command, which allows remote attackers to obtain potentially sensitive information by sending this command to the daemon's TCP port.",
					"title": "Vulnerability Description",
					"type": "General"
				}
			],
			"cve": "CVE-2009-1494",
			"product_statuses": [
				{
