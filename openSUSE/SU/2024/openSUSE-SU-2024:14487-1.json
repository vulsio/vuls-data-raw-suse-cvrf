	"vulnerability": [
		{
			"notes": [
				{
					"text": "gio/gsocks4aproxy.c in GNOME GLib before 2.82.1 has an off-by-one error and resultant buffer overflow because SOCKS4_CONN_MSG_LEN is not sufficient for a trailing '\\0' character.",
					"title": "Vulnerability Description",
					"type": "General"
				}
			],
