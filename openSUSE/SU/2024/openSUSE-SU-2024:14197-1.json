	"vulnerability": [
		{
			"notes": [
				{
					"text": "Due to large allocation checks in Angle for GLSL shaders being too lenient an out-of-bounds access could occur when allocating more than 8192 ints in private shader memory on mac OS. This vulnerability affects Firefox < 128, Firefox ESR < 115.13, Thunderbird < 115.13, and Thunderbird < 128.",
					"title": "Vulnerability Description",
					"type": "General"
				}
			],
