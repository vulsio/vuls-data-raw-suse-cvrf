	"vulnerability": [
		{
			"notes": [
				{
					"text": "Multiple integer overflows in glib/gbase64.c in GLib before 2.20 allow context-dependent attackers to execute arbitrary code via a long string that is converted either (1) from or (2) to a base64 representation.",
					"title": "Vulnerability Description",
					"type": "General"
				}
			],
