	"vulnerability": [
		{
			"notes": [
				{
					"text": "OpenSC before 0.11.5 uses weak permissions (ADMIN file control information of 00) for the 5015 directory on smart cards and USB crypto tokens running Siemens CardOS M4, which allows physically proximate attackers to change the PIN.",
					"title": "Vulnerability Description",
					"type": "General"
				}
			],
			"cve": "CVE-2008-2235",
			"product_statuses": [
				{
					"type": "Fixed",
					"product_id": [
