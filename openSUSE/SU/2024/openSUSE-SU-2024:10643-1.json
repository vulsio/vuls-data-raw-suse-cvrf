	"vulnerability": [
		{
			"notes": [
				{
					"text": "Avahi before 0.6.15 does not verify the sender identity of netlink messages to ensure that they come from the kernel instead of another process, which allows local users to spoof network changes to Avahi.",
					"title": "Vulnerability Description",
					"type": "General"
				}
			],
