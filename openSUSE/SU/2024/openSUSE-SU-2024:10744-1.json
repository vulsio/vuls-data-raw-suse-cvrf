	"vulnerability": [
		{
			"notes": [
				{
					"text": "Camel (camel-imap-folder.c) in the mailer component for Evolution Data Server 1.11 allows remote IMAP servers to execute arbitrary code via a negative SEQUENCE value in GData, which is used as an array index.",
					"title": "Vulnerability Description",
					"type": "General"
				}
			],
			"cve": "CVE-2007-3257",
