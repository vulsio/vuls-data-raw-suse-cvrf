	"vulnerability": [
		{
			"notes": [
				{
					"text": "A flaw was found in the keylime attestation verifier, which fails to flag a device's submitted TPM quote as faulty when the quote's signature does not validate for some reason. Instead, it will only emit an error in the log without flagging the device as untrusted.",
					"title": "Vulnerability Description",
					"type": "General"
				}
			],
