	"document_notes": [
		{
			"text": "Security update for libressl",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "libressl was updated to fix two security issues.\n\nThese security issues were fixed:\n- CVE-2015-5333: Memory leak when decoding X.509 certificates (boo#950707)\n- CVE-2015-5334: Buffer overflow when decoding X.509 certificates (boo#950708)\n  ",
			"title": "Details",
			"type": "General"
		},
