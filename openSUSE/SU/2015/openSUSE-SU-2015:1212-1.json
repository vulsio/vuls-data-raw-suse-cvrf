	"document_notes": [
		{
			"text": "Security update for libwmf",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "libwmf was updated to fix four security issues.\n\nThese security issues were fixed:\n- CVE-2015-4588: Heap overflow (bnc#933109).\n- CVE-2015-4696: Use after free (bnc#936062).\n- CVE-2015-4695: Heap buffer over read (bnc#936058).\n- CVE-2015-0848: Heap overflow (bnc#933109).\n  ",
			"title": "Details",
			"type": "General"
		},
