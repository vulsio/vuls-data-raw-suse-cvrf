	"vulnerability": [
		{
			"notes": [
				{
					"text": "Integer overflow in kbdint.c in mod_sftp in ProFTPD 1.3.4d and 1.3.5r3 allows remote attackers to cause a denial of service (memory consumption) via a large response count value in an authentication request, which triggers a large memory allocation.",
					"title": "Vulnerability Description",
					"type": "General"
				}
			],
