	"document_notes": [
		{
			"text": "Security update for ffmpeg",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "\nThis update to ffmpeg 2.8.3 fixes the following security issues:\n\n* CVE-2015-8363: Check for duplicate SIZ marker / asan_heap-oob [boo#957114]\n* CVE-2015-8364: Check image dimensions / integer overflow [boo#957115]\n* CVE-2015-8365: out of array access / asan_heap-oob [boo#957116]\n",
			"title": "Details",
			"type": "General"
		},
