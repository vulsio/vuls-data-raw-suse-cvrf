	"vulnerability": [
		{
			"notes": [
				{
					"text": "In versions 4.18 and earlier of the Eclipse Platform, the Help Subsystem does not authenticate active help requests to the local help web server, allowing an unauthenticated local attacker to issue active help commands to the associated Eclipse Platform process or Eclipse Rich Client Platform process.",
					"title": "Vulnerability Description",
					"type": "General"
				}
			],
