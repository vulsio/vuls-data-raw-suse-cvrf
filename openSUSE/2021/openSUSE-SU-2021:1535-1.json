	"document_notes": [
		{
			"text": "Security update for ruby2.5",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for ruby2.5 fixes the following issues:\n\n- CVE-2021-31799: Fixed Command injection vulnerability in RDoc (bsc#1190375).\n- CVE-2021-31810: Fixed trusting FTP PASV responses vulnerability in Net:FTP (bsc#1188161).\n- CVE-2021-32066: Fixed StartTLS stripping vulnerability in Net:IMAP (bsc#1188160).\n\nThis update was imported from the SUSE:SLE-15:Update update project.",
			"title": "Details",
			"type": "General"
		},
