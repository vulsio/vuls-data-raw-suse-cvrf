	"document_notes": [
		{
			"text": "Security update for pcre",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for pcre fixes the following issues:\n\nUpdate pcre to version 8.45:\n\n- CVE-2020-14155: Fixed integer overflow via a large number after a '(?C' substring (bsc#1172974).\n- CVE-2019-20838: Fixed buffer over-read in JIT compiler (bsc#1172973)\n\nThis update was imported from the SUSE:SLE-15:Update update project.",
			"title": "Details",
			"type": "General"
		},
