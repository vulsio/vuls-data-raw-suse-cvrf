	"document_notes": [
		{
			"text": "Security update for gimp",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for gimp fixes the following issues:\n\n- CVE-2017-17784: Fixed an insufficient string validation for input names (bsc#1073624).\n- CVE-2017-17785: Fixed an heap-based buffer overflow in FLI import (bsc#1073625).\n- CVE-2017-17786: Fixed an out-of-bounds read in TGA (bsc#1073626).\n\nThis update was imported from the SUSE:SLE-15:Update update project.",
			"title": "Details",
			"type": "General"
		},
