	"vulnerability": [
		{
			"notes": [
				{
					"text": "backend/comics/comics-document.c (aka the comic book backend) in GNOME Evince before 3.24.1 allows remote attackers to execute arbitrary commands via a .cbt file that is a TAR archive containing a filename beginning with a \"--\" command-line option substring, as demonstrated by a --checkpoint-action=exec=bash at the beginning of the filename.",
					"title": "Vulnerability Description",
					"type": "General"
				}
			],
