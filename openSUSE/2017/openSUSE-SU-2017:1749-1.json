	"document_notes": [
		{
			"text": "Security update for kdepim4",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for kdepim4 fixes the following issues:\n\n- CVE-2017-9604: The kmail 'send later' function does not have 'sign/encryption' action ensured. (boo#1044210)\n\nThe package kdepim-addons was updated to conflict with 4.x based akonadi package to prevent file conflicts. (boo#1045936)\n",
			"title": "Details",
			"type": "General"
		},
