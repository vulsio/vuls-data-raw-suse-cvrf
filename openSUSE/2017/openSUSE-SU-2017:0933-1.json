	"document_notes": [
		{
			"text": "Security update for ruby2.2, ruby2.3",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "\nThis update for ruby2.2, ruby2.3 fixes the following issues:\n\nSecurity issues fixed:\n- CVE-2016-2339: heap overflow vulnerability in the Fiddle::Function.new'initialize' (boo#1018808)\n- CVE-2015-7551: Unsafe tainted string usage in Fiddle and DL (boo#959495)\n\nDetailed ChangeLog:\n- http://svn.ruby-lang.org/repos/ruby/tags/v2_2_6/ChangeLog\n- http://svn.ruby-lang.org/repos/ruby/tags/v2_3_3/ChangeLog\n",
			"title": "Details",
			"type": "General"
		},
