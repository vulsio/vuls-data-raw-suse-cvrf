	"document_notes": [
		{
			"text": "Security update for libnettle",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for libnettle fixes the following issues:\n\n-  CVE-2016-6489:\n  * Reject invalid RSA keys with even modulo.\n  * Check for invalid keys, with even p, in dsa_sign().\n  * Use function mpz_powm_sec() instead of mpz_powm() (bsc#991464).\n\nThis update was imported from the SUSE:SLE-12:Update update project.",
			"title": "Details",
			"type": "General"
		},
