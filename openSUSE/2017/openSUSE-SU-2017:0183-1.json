	"document_notes": [
		{
			"text": "Security update for pdns",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for pdns fixes the following issues:\n\n- CVE-2016-2120: Crafted zone record could have caused a denial of service (bsc#1018329).\n- CVE-2016-7068: Crafted queries could have caused abnormal CPU usage (bsc#1018326).\n- CVE-2016-7072: Denial of service via the web server (bsc#1018327).\n- CVE-2016-7073: Fixed insufficient validation of TSIG signatures (bsc#1018328).\n- CVE-2016-7074: Fixed insufficient validation of TSIG signatures ((bsc#1018328).\n",
			"title": "Details",
			"type": "General"
		},
