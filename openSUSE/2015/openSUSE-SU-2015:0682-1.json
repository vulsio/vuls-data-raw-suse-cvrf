	"document_notes": [
		{
			"text": "Security update for Chromium",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "Chromium was updated to 41.0.2272.118 to fix two security issues.\n\nThe following vulnerabilities were fixed:\n\n\n* A combination of V8, Gamepad and IPC bugs could lead to remote code execution outside of the sandbox (CVE-2015-1233, boo#925713)\n* Buffer overflow via race condition in GPU (CVE-2015-1234, boo#925714)",
			"title": "Details",
			"type": "General"
		},
