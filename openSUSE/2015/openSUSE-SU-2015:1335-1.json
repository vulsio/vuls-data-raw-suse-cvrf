	"document_notes": [
		{
			"text": "Security update for bind",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "bind was updated to fix one security issue.\n\nThis security issue was fixed:\n- CVE-2015-5477: Remote DoS via TKEY queries (boo#939567)\n\nExposure to this issue can not be prevented by either ACLs or configuration options limiting or denying service because the exploitable code occurs early in the packet handling.\n",
			"title": "Details",
			"type": "General"
		},
