	"document_notes": [
		{
			"text": "Security update for SDL2_image",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for SDL2_image fixes the following issues:\n\nSecurity issues fixed:\n\n- CVE-2018-3839: Fixed an exploitable code execution vulnerability that existed in the XCF image rendering functionality of the Simple DirectMedia Layer (bsc#1089087).\n- CVE-2018-3977: Fixed a possible code execution via creafted XCF image that could have caused a heap overflow (bsc#1114519).\n",
			"title": "Details",
			"type": "General"
		},
