	"document_notes": [
		{
			"text": "Security update for libraw",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for libraw fixes the following issues:\n\n* CVE-2018-10528: A stack-based buffer overflow in the utf2char function in libraw_cxx.cpp was fixed. [boo#1091345]\n* CVE-2018-10529: A out-of-bounds read affecting the X3F property table list implementation in libraw_x3f.cpp and libraw_cxx.cpp was fixed. [boo#1091346]\n",
			"title": "Details",
			"type": "General"
		},
