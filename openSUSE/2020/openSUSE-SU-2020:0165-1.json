	"document_notes": [
		{
			"text": "Security update for wicked",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for wicked fixes the following issues:\n\n- CVE-2019-18902: Fixed a use-after-free when receiving invalid DHCP6 client options (bsc#1160903).\n- CVE-2020-7216: Fixed a potential denial of service via a memory leak when processing packets with missing message type option in DHCP4 (bsc#1160905).\n\nThis update was imported from the SUSE:SLE-15-SP1:Update update project.",
			"title": "Details",
			"type": "General"
		},
