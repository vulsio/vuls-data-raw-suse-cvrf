	"document_notes": [
		{
			"text": "Recommended update for libtorrent-rasterbar",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "\nThis update for libtorrent-rasterbar fixes the following issues:\n\n- Update to version 1.0.10:\n  * Fix inverted priority of incoming piece suggestions.\n  * Fix a crash on invalid input in http_parser.\n  * Add a new 'preformatted' type to bencode entry variant type.\n  * Fix division by zero in super-seeding logic\n\n\n",
			"title": "Details",
			"type": "General"
		},
