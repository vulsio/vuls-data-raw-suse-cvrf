	"document_notes": [
		{
			"text": "Security update for vim",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for vim fixes the following issues:\n\n- CVE-2023-5344: Heap-based Buffer Overflow in vim prior to 9.0.1969 (bsc#1215940)\n- CVE-2023-5441: segfault in exmode when redrawing (bsc#1216001)\n- CVE-2023-5535: use-after-free from buf_contents_changed() (bsc#1216167)\n- CVE-2023-46246: Integer Overflow in :history command (bsc#1216696)\n",
			"title": "Details",
			"type": "General"
		},
