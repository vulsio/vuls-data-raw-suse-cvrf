	"document_notes": [
		{
			"text": "Security update for rubygem-rack",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for rubygem-rack fixes the following issues:\n\n- CVE-2022-44570: Fixed a potential denial of service when parsing a\n  RFC2183 multipart boundary (bsc#1207597).\n- CVE-2022-44571: Fixed a potential denial of service when parsing a\n  Range header (bsc#1207599).\n- CVE-2022-44572: Fixed a potential denial of service when parsing a\n  Content-Disposition header (bsc#1207596).\n",
			"title": "Details",
			"type": "General"
		},
