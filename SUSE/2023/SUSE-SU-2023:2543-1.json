	"document_notes": [
		{
			"text": "Security update for kubernetes1.23",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for kubernetes1.23 fixes the following issues:\n\n- CVE-2023-2727: Fixed bypassing policies imposed by the ImagePolicyWebhook admission plugin (bsc#1211630).\n- CVE-2023-2728: Fixed bypassing enforce mountable secrets policy imposed by the ServiceAccount admission plugin (bsc#1211631).\n",
			"title": "Details",
			"type": "General"
