	"document_notes": [
		{
			"text": "Security update for helm",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for helm fixes the following issues:\n\nUpdate to version 3.11.2:\n\n* chore(deps): bump github.com/rubenv/sql-migrate from 1.2.0 to 1.3.1\n* the linter varcheck and deadcode are deprecated (since v1.49.0)\n* fix template --output-dir issue\n* build against a supported go version: go1.19 (bsc#1209670)\n",
			"title": "Details",
			"type": "General"
		},
