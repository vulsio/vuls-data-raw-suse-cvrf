	"document_notes": [
		{
			"text": "Security update for djvulibre",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for djvulibre fixes the following issues:\n\n- CVE-2021-46310: Fixed divide by zero in IW44Image.cpp (bsc#1214670).\n- CVE-2021-46312: Fixed divide by zero in IW44EncodeCodec.cpp (bsc#1214672).\n- CVE-2021-32490: Fixed out of bounds write in function DJVU:filter_bv() via crafted djvu file (bsc#1185895).\n",
			"title": "Details",
			"type": "General"
		},
