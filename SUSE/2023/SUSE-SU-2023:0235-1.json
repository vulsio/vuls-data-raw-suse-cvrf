	"document_notes": [
		{
			"text": "Security update for the Linux Kernel (Live Patch 35 for SLE 15 SP1)",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for the Linux Kernel 4.12.14-150100_197_126 fixes one issue.\n\nThe following security issue was fixed:\n\n- CVE-2022-3424: Fixed use-after-free in gru_set_context_option(), gru_fault() and gru_handle_user_call_os() that could lead to kernel panic  (bsc#1204167).\n",
			"title": "Details",
			"type": "General"
		},
