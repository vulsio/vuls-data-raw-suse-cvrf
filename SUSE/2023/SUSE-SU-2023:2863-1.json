	"document_notes": [
		{
			"text": "Security update for java-1_8_0-ibm",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for java-1_8_0-ibm fixes the following issues:\n\n  Updated to Java 8.0 Service Refresh 8 Fix Pack 6 (bsc#1213000):\n\n  - Fixed issue in Java Virtual Machine where outofmemory (OOM) killer terminates the jvm due to failure in control groups detection.\n",
			"title": "Details",
			"type": "General"
