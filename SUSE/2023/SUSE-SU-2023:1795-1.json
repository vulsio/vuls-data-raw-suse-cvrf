	"document_notes": [
		{
			"text": "Security update for openvswitch",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for openvswitch fixes the following issues:\n\n- CVE-2021-36980: Fixed a use-after-free issue during the decoding of\n  a RAW_ENCAP action (bsc#1188524).\n- CVE-2022-32166: Fixed a out of bounds read in minimask_equal() (bsc#1203865).\n",
			"title": "Details",
			"type": "General"
		},
