	"document_notes": [
		{
			"text": "Security update for apache2",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for apache2 fixes the following issues:\n\n- CVE-2023-31122: Fixed an out of bounds read in mod_macro (bsc#1216424).\n\nNon-security fixes:\n\n-  Fixed the content type handling in mod_proxy_http2 (bsc#1214357).\n-  Fixed a floating point exception crash (bsc#1207399).\n",
			"title": "Details",
			"type": "General"
		},
