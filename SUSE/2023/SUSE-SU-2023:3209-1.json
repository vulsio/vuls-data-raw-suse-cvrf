	"document_notes": [
		{
			"text": "Security update for libqt5-qtsvg",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for libqt5-qtsvg fixes the following issues:\n\n- CVE-2021-45930: Fixed an out-of-bounds write that may have lead to a denial-of-service (bsc#1196654).\n- CVE-2023-32573: Fixed missing initialization of QtSvg QSvgFont m_unitsPerEm variable (bsc#1211298).\n",
			"title": "Details",
			"type": "General"
		},
