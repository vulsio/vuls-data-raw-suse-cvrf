	"document_notes": [
		{
			"text": "Security update for netty, netty-tcnative",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for netty, netty-tcnative fixes the following issues:\n\n- Updated netty to version 4.1.100:\n\n  - CVE-2023-44487: Fixed a potential denial of service scenario via\n    RST frame floods (bsc#1216169).\n\n- Updated netty-tcnative to version 2.0.62 Final.\n",
			"title": "Details",
			"type": "General"
		},
