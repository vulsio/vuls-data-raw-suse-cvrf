	"document_notes": [
		{
			"text": "Security update for gnutls",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for gnutls fixes the following issues:\n\n- CVE-2024-0567: Fixed an incorrect rejection of certificate chains\n  with distributed trust (bsc#1218862).\n- CVE-2024-0553: Fixed a timing attack against the RSA-PSK key\n  exchange, which could lead to the leakage of sensitive data\n  (bsc#1218865).\n",
			"title": "Details",
			"type": "General"
		},
