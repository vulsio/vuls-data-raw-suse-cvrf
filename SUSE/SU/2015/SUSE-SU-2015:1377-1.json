	"document_notes": [
		{
			"text": "Security update for cups-filters",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "cups-filters was updated to fix two security issues.\n\nThese security issues were fixed:\n- CVE-2015-3279: Texttopdf integer overflow (incomplete fix for CVE-2015-3258) (bsc#937018).\n- CVE-2015-3258: Texttopdf heap-based buffer overflow (bsc#936281).\n  ",
			"title": "Details",
			"type": "General"
