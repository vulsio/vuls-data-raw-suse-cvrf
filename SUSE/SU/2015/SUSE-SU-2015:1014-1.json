	"document_notes": [
		{
			"text": "Security update for vorbis-tools",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "\nVorbis tools was updated to fix division by zero and integer overflows by crafted WAV files (CVE-2014-9638, CVE-2014-9639, bnc#914439, bnc#914441),\nthat would allow attackers to crash the vorbis tools processes.\n",
			"title": "Details",
			"type": "General"
		},
