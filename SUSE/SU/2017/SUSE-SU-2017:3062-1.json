	"document_notes": [
		{
			"text": "Security update for gimp",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "\n  \nThis update for gimp fixes the following issues:\n\n- Don't build gimp with webkit1 support, as it is no longer maintained and has\n  plenty of security bugs. \n\n  This disables the GIMP's built-in help browser; it will use an external browser when configured this way.\n\n  This works around a number of security vulnerabilities in Webkit1.\n",
			"title": "Details",
			"type": "General"
		},
