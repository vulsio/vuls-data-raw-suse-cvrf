	"document_notes": [
		{
			"text": "Security update for libvirt",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for libvirt fixes several issues.\n\nThis security issue was fixed:\n\n- bsc#1053600: Escape ssh commed line to prevent interpreting malicious\n  hostname as arguments, allowing for command execution\n\nThese non-security issues were fixed:\n\n- bsc#1025340: Use xend for nodeGetFreeMemory API\n- bsc#1026236: Add support for tsc timers on xen\n",
			"title": "Details",
			"type": "General"
		},
