	"document_notes": [
		{
			"text": "Security update for bash",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "\nThis update for bash fixed several issues\n\nThis security issue was fixed:\n\n- CVE-2016-9401: popd in bash might allowed local users to bypass the restricted shell and cause a use-after-free via a crafted address (bsc#1010845).\n\nThis non-security issue was fixed:\n\n- Fix when HISTSIZE=0 and chattr +a .bash_history (bsc#1031729)\n",
			"title": "Details",
			"type": "General"
		},
