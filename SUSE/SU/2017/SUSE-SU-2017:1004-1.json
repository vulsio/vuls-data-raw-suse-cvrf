	"document_notes": [
		{
			"text": "Security update for gstreamer-plugins-good",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "\nThis update for gstreamer-plugins-good fixes the following issues:\n\n- A crafted aac audio file could have caused an invalid read and thus\n  corruption or denial of service (bsc#1024014, CVE-2016-10198)\n- A crafted mp4 file could have caused an invalid read and thus corruption or\n  denial of service (bsc#1024017, CVE-2016-10199)\n- A crafted avi file could have caused an invalid read and thus corruption or\n  denial of service (bsc#1024034, CVE-2017-5840)\n",
			"title": "Details",
			"type": "General"
		},
