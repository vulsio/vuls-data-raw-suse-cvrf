	"document_notes": [
		{
			"text": "Security update for java-1_8_0-openjdk",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for java-1_8_0-openjdk to version jdk8u201 (icedtea 3.11.0) fixes the following issues:\n\t  \nSecurity issues fixed: \n\n- CVE-2019-2422: Fixed a memory disclosure in FileChannelImpl (bsc#1122293).\n- CVE-2018-11212: Fixed an issue in alloc_sarray function in jmemmgr.c (bsc#1122299).\t\n\nComplete list of changes: https://mail.openjdk.java.net/pipermail/distro-pkg-dev/2019-March/041223.html \n",
			"title": "Details",
			"type": "General"
		},
