	"document_notes": [
		{
			"text": "Security update for webkit2gtk3",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for webkit2gtk3 to version 2.24.0 fixes the following issue:\n\nSecurity issue fixed: \n\n- CVE-2019-8375: Fixed an issue in UIProcess subsystem which could allow the script dialog\n  size to exceed the web view size leading to Buffer Overflow or other unspecified impact (bsc#1126768).\n",
			"title": "Details",
			"type": "General"
