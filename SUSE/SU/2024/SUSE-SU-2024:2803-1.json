	"document_notes": [
		{
			"text": "Security update for ffmpeg-4",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for ffmpeg-4 fixes the following issues:\n\n- CVE-2024-32230: Fixed buffer overflow due to negative-size-param bug in load_input_picture() (bsc#1227296).\n- CVE-2023-51798: Fixed floating point exception in the via the interpolate function in libavfilter/vf_minterpolate.c (bsc#1223304).\n",
			"title": "Details",
			"type": "General"
		},
