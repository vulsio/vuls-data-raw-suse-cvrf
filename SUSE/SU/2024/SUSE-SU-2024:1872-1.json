	"document_notes": [
		{
			"text": "Security update for python-tqdm",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for python-tqdm fixes the following issues:\n\nUpdate to version 4.66.4:\n\n* rich: fix completion\n* cli: eval safety (CVE-2024-34062, bsc#1223880)\n* pandas: add DataFrame.progress_map\n* notebook: fix HTML padding\n* keras: fix resuming training when verbose>=2\n* fix format_num negative fractions missing leading zero\n* fix Python 3.12 DeprecationWarning on import\n",
			"title": "Details",
			"type": "General"
		},
