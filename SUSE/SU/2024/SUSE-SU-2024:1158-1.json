	"document_notes": [
		{
			"text": "Security update for kubevirt, virt-api-container, virt-controller-container, virt-exportproxy-container, virt-exportserver-container, virt-handler-container, virt-launcher-container, virt-libguestfs-tools-container, virt-operator-container, virt-pr-helper-container",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for kubevirt, virt-api-container, virt-controller-container, virt-exportproxy-container, virt-exportserver-container, virt-handler-container, virt-launcher-container, virt-libguestfs-tools-container, virt-operator-container, virt-pr-helper-container fixes the following issues:\n\n- rebuild against current security updates\n\n- Install qemu-hw-usb-host to enable USB passthrough (bsc#1221538)\n- Group together arch specific parts of the code\n- Cleanup after writing config files with augtool\n",
			"title": "Details",
			"type": "General"
		},
