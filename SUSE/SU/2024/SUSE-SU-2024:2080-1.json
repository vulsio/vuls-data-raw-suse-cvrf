	"document_notes": [
		{
			"text": "Security update for libzypp, zypper",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for libzypp, zypper fixes the following issues:\n\n- CVE-2017-9271: Fixed proxy credentials written to log files (bsc#1050625).\n\nThe following non-security bugs were fixed:\n\n- clean: Do not report an error if no repos are defined at all (bsc#1223971)\n",
			"title": "Details",
			"type": "General"
		},
