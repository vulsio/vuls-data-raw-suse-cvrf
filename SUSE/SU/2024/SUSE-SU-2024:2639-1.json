	"document_notes": [
		{
			"text": "Security update for kubevirt, virt-api-container, virt-controller-container, virt-exportproxy-container, virt-exportserver-container, virt-handler-container, virt-launcher-container, virt-libguestfs-tools-container, virt-operator-container, virt-pr-helper-container",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for kubevirt, virt-api-container, virt-controller-container, virt-exportproxy-container, virt-exportserver-container, virt-handler-container, virt-launcher-container, virt-libguestfs-tools-container, virt-operator-container, virt-pr-helper-container fixes the following issues:\n\n- Update to version 1.2.2\n\n  Release notes https://github.com/kubevirt/kubevirt/releases/tag/v1.2.2\n  Release notes https://github.com/kubevirt/kubevirt/releases/tag/v1.2.1\n  Release notes https://github.com/kubevirt/kubevirt/releases/tag/v1.2.0\n\n- Use predefined configuration files for libvirt\n- Install psmisc (provides killall for tests)\n",
			"title": "Details",
			"type": "General"
		},
