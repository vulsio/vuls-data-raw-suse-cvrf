	"vulnerability": [
		{
			"notes": [
				{
					"text": "ModularSquareRoot in Crypto++ (aka cryptopp) through 8.9.0 allows attackers to cause a denial of service (infinite loop) via crafted DER public-key data associated with squared odd numbers, such as the square of 268995137513890432434389773128616504853.",
					"title": "Vulnerability Description",
					"type": "General"
				}
			],
