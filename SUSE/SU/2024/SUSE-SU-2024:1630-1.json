	"document_notes": [
		{
			"text": "Security update for perl",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for perl fixes the following issues:\n\nSecurity issues fixed:\n\n- CVE-2018-6913: Fixed space calculation issues in pp_pack.c (bsc#1082216)\n- CVE-2018-6798: Fixed heap buffer overflow in regexec.c (bsc#1082233)\n- CVE-2023-31484: Enabled TLS certificate verification in CPAN (bsc#1210999)\n- CVE-2017-6512: Fixed File::Path rmtree/remove_tree race condition (bsc#1047178)\n",
			"title": "Details",
			"type": "General"
		},
