	"document_notes": [
		{
			"text": "Security update for ImageMagick",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for ImageMagick fixes the following issues:\n\n- CVE-2020-27560: Fixed potential denial of service in OptimizeLayerFrames function in MagickCore/layer.c (bsc#1178067).\n- Fixed greyish image produced by incorrect colorspace (bsc#1106272).\n",
			"title": "Details",
			"type": "General"
