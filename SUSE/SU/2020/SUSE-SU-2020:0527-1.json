	"document_notes": [
		{
			"text": "Security update for mariadb",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for mariadb fixes the following issues:\n\nMariaDB was updated to version 10.0.40-3 (bsc#1162388).\n\nSecurity issues fixed:\n\n- CVE-2020-2574: Fixed a difficult to exploit vulnerability that allowed an attacker to crash the client (bsc#1162388).\n- CVE-2019-18901: Fixed an unsafe path handling behavior in mysql-systemd-helper (bsc#1160895).\n",
			"title": "Details",
			"type": "General"
		},
