	"document_notes": [
		{
			"text": "Security update for sysstat",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for sysstat fixes the following issues:\n\nSecurity issue fixed:                                                                                                                                                               \n                                                                                                                                                                                   \n- CVE-2019-19725: Fixed double free in check_file_actlst in sa_common.c (bsc#1159104).                                                                                    \n\nBug fixes:\n\n- Enable log information of starting/stoping services. (bsc#1144923, jsc#SLE-5958)\n",
			"title": "Details",
			"type": "General"
		},
