	"document_notes": [
		{
			"text": "Security update for slurm",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for slurm fixes the following issues:\n\n- CVE-2019-19727: Fix permissions of configuration file 'slurmdbd.conf' (bsc#1155784).\n- Fix ownership of /var/spool/slurm on new installations and upgrade (bsc#1158696).\n- Fix '%posttrans' macro to cope with added newline (bsc#1153259).\n",
			"title": "Details",
			"type": "General"
		},
