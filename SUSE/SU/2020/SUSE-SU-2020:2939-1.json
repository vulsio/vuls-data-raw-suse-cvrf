	"document_notes": [
		{
			"text": "Security update for crmsh",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for crmsh fixes the following issues:\n\n- Fixed start_delay with start-delay(bsc#1176569)\n- fix on_fail should be on-fail(bsc#1176569)\n- config: Try to handle configparser.MissingSectionHeaderError while reading config file\n- ui_configure: Obscure sensitive data by default(bsc#1163581\n- hb_report: collect archived logs(bsc#1148873, bsc#1176441)\n",
			"title": "Details",
			"type": "General"
		},
