	"document_notes": [
		{
			"text": "Security update for wireshark",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for wireshark fixes the following issues:\n\n- wireshark was updated to 3.2.8:\n  - CVE-2020-26575: Fixed an issue where FBZERO dissector was entering in infinite loop (bsc#1177406)\n  - CVE-2020-28030: Fixed an issue where GQUIC dissector was crashing (bsc#1178291)\n  * Infinite memory allocation while parsing this tcp packet\n",
			"title": "Details",
			"type": "General"
		},
