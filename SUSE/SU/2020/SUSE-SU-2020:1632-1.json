	"document_notes": [
		{
			"text": "Security update for xen",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for xen to version 4.11.4 fixes the following issues:\n\n- CVE-2020-0543: Fixed a side channel attack against special registers which could have resulted in leaking of read values to cores other than the one which called it.\n  This attack is known as Special Register Buffer Data Sampling (SRBDS) or 'CrossTalk' (bsc#1172205).\n",
			"title": "Details",
			"type": "General"
		},
