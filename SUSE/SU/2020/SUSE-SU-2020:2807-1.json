	"vulnerability": [
		{
			"notes": [
				{
					"text": "libaspell.a in GNU Aspell before 0.60.8 has a buffer over-read for a string ending with a single '\\0' byte, if the encoding is set to ucs-2 or ucs-4 outside of the application, as demonstrated by the ASPELL_CONF environment variable.",
					"title": "Vulnerability Description",
					"type": "General"
				}
			],
