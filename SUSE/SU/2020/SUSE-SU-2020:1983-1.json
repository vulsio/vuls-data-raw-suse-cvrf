	"document_notes": [
		{
			"text": "Security update for tomcat",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for tomcat fixes the following issues:\n\t  \nTomcat was updated to 9.0.36 See changelog at\n\n- CVE-2020-11996: Fixed an issue which by sending a specially crafted sequence of HTTP/2 requests could have triggered high CPU\n  usage for several seconds making potentially the server unresponsive (bsc#1173389).\n",
			"title": "Details",
			"type": "General"
		},
