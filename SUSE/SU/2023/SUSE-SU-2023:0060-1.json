	"vulnerability": [
		{
			"notes": [
				{
					"text": "Multiple heap buffer overflows in tiffcrop.c utility in libtiff library Version 4.4.0 allows attacker to trigger unsafe or out of bounds memory access via crafted TIFF image file which could result into application crash, potential information disclosure or any other context-dependent impact",
					"title": "Vulnerability Description",
					"type": "General"
				}
			],
