	"document_notes": [
		{
			"text": "Security update for gstreamer-plugins-bad",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for gstreamer-plugins-bad fixes the following issues:\n\n- CVE-2023-40474: Fixed integer overflow causing out of bounds writes when handling invalid uncompressed video (bsc#1215796).\n- CVE-2023-40476: Fixed possible overflow using max_sub_layers_minus1 (bsc#1215793).\n",
			"title": "Details",
			"type": "General"
		},
