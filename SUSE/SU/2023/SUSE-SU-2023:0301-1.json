	"document_notes": [
		{
			"text": "Security update for sssd",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for sssd fixes the following issues:\n\n- CVE-2022-4254: Fixed a bug in libsss_certmap which could allow an attacker to gain control of the admin account and perform a full domain takeover. (bsc#1207474)\n- Move systemd RPM macros managing the service from 'sssd-common' to 'sssd' package (bsc#1203583)\n",
			"title": "Details",
			"type": "General"
		},
