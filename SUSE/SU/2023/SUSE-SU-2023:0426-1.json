	"document_notes": [
		{
			"text": "Security update for git",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for git fixes the following issues:\n\n  - CVE-2023-22490: Fixed incorrectly usable local clone optimization even when using a non-local transport (bsc#1208027).\n  - CVE-2023-23946: Fixed issue where a path outside the working tree can be overwritten as the user who is running 'git apply' (bsc#1208028).\n",
			"title": "Details",
			"type": "General"
		},
