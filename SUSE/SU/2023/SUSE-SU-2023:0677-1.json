	"document_notes": [
		{
			"text": "Security update for poppler",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for poppler fixes the following issues:\n\n  - CVE-2022-38784: Fixed integer overflow in the JBIG2 decoder (bsc#1202692).\n\n  Bugfixes:\n\n  - Fixed issue where some PDF generators generate PDF with some wrong numbers in entry table, but the content is still valid (bsc#1181551).\n\n",
			"title": "Details",
			"type": "General"
		},
