	"document_notes": [
		{
			"text": "Security update for qemu",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for qemu fixes the following issues:\n\n- CVE-2021-4207: Fixed double fetch in qxl_cursor() that could lead to heap buffer overflow (bsc#1198037).\n- CVE-2023-0330: Fixed DMA reentrancy issue that could lead to stack overflow (bsc#1207205).\n- CVE-2023-2861: Fixed improper access control on special files (bsc#1212968).\n",
			"title": "Details",
			"type": "General"
		},
