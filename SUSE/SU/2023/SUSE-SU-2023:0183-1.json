	"vulnerability": [
		{
			"notes": [
				{
					"text": "A carefully crafted If: request header can cause a memory read, or write of a single zero byte, in a pool (heap) memory location beyond the header value sent. This could cause the process to crash.\n\nThis issue affects Apache HTTP Server 2.4.54 and earlier.\n",
					"title": "Vulnerability Description",
					"type": "General"
				}
			],
