	"document_notes": [
		{
			"text": "Security update for dpdk",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for dpdk fixes the following issues:\n\n- CVE-2022-2132: Fixed DoS when a vhost header crosses more than two descriptors and exhausts all mbufs (bsc#1202903).    \n- CVE-2022-28199: Fixed buffer overflow in the vhost code (bsc#1202956).\n\n",
			"title": "Details",
			"type": "General"
		},
