	"document_notes": [
		{
			"text": "Security update for zsh",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for zsh fixes the following issues:\n\n- CVE-2018-0502: Fixed execve call vulnerability to program named on the second line when the beginning of a #! script file was mishandled. (bsc#1107296, bsc#1107294)\n- CVE-2018-13259: Fixed execve call vulnerability to program name that is a substring of the intended one. (bsc#1107296, bsc#1107294)\n",
			"title": "Details",
			"type": "General"
		},
