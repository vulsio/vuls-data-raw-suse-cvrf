	"document_notes": [
		{
			"text": "Security update for logrotate",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for logrotate fixes the following issues:\n\nSecurity issues fixed:\n\n- Improved coredump handing for SUID binaries (bsc#1192449).\n\nNon-security issues fixed:\n\n- Fixed 'logrotate emits unintended warning: keyword size not properly separated, found 0x3d' (bsc#1200278, bsc#1200802).\n",
			"title": "Details",
			"type": "General"
		},
