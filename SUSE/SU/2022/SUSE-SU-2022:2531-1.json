	"document_notes": [
		{
			"text": "Security update for java-1_8_0-openjdk",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for java-1_8_0-openjdk fixes the following issues:\n\nUpdate to version jdk8u332 - April 2022 CPU (icedtea-3.23.0)\n\n- CVE-2022-21426: Better XPath expression handling (bsc#1198672)\n- CVE-2022-21443: Improved Object Identification (bsc#1198675)\n- CVE-2022-21434: Better invocation handler handling (bsc#1198674)\n- CVE-2022-21476: Improve Santuario processing (bsc#1198671)\n- CVE-2022-21496: Improve URL supports (bsc#1198673)\n\nAnd further Security fixes, Import of OpenJDK 8 u332, Backports and Bug fixes.\n",
			"title": "Details",
			"type": "General"
		},
