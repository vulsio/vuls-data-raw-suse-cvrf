	"document_notes": [
		{
			"text": "Security update for ucode-intel",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for ucode-intel fixes the following issues:\n\nUpdated to Intel CPU Microcode 20220510 release. (bsc#1199423)\nUpdated to Intel CPU Microcode 20220419 release. (bsc#1198717)\n\n- CVE-2022-21151: Processor optimization removal or modification of security-critical code for some Intel(R) Processors may allow an authenticated user to potentially enable information disclosure via local access (bsc#1199423).\n",
			"title": "Details",
			"type": "General"
		},
