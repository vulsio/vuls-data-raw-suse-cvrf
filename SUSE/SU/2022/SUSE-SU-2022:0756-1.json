	"vulnerability": [
		{
			"notes": [
				{
					"text": "An issue was discovered in fs/gfs2/rgrp.c in the Linux kernel before 4.8. A use-after-free is caused by the functions gfs2_clear_rgrpd and read_rindex_entry.",
					"title": "Vulnerability Description",
					"type": "General"
