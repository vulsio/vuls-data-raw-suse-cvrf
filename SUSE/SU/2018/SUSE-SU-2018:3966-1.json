	"document_notes": [
		{
			"text": "Security update for glib2",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for glib2 fixes the following issues:\n\nSecurity issues fixed:\n\n- CVE-2018-16429: Fixed out-of-bounds read vulnerability ing_markup_parse_context_parse() (bsc#1107116).\n- Fixing potentially exploitable bugs in UTF-8 validation in Variant and DBUS message parsing (bsc#1111499).\n",
			"title": "Details",
			"type": "General"
		},
