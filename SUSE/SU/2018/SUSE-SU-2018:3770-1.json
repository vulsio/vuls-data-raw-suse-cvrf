	"document_notes": [
		{
			"text": "Security update for postgresql10",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for postgresql10 fixes the following issues:\n\nSecurity issue fixed:\n\n- CVE-2018-16850: Fixed improper quoting of transition table names when pg_dump emits CREATE TRIGGER could have caused privilege escalation (bsc#1114837).\n\nNon-security issues fixed:\n\n- Update to release 10.6:\n  * https://www.postgresql.org/docs/current/static/release-10-6.html\n",
			"title": "Details",
			"type": "General"
		},
