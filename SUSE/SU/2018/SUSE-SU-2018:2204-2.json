	"document_notes": [
		{
			"text": "Security update for libsoup",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for libsoup fixes the following issues:\n\nSecurity issue fixed:\n\n- CVE-2018-12910: Fix crash when handling empty hostnames (bsc#1100097).\n- CVE-2017-2885: Fix chunk decoding buffer overrun that could be exploited against either clients or servers (bsc#1052916).\n\nBug fixes:\n\n- bsc#1086036: translation-update-upstream commented out for Leap\n",
			"title": "Details",
			"type": "General"
		},
