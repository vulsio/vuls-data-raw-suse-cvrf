	"document_notes": [
		{
			"text": "Security update for python3",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for python3 fixes the following issues:\n\nSecurity issue fixed:\n\n- CVE-2017-18207: Fixed possible denial of service vulnerability by adding a check to Lib/wave.py that verifies that at least one channel is provided (bsc#1083507).\n\nBug fixes:\n\n- Require python-Sphinx-latex for building on Leap 42.3 or newer.\n",
			"title": "Details",
			"type": "General"
		},
