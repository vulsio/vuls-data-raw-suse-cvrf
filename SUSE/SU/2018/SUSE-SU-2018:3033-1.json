	"document_notes": [
		{
			"text": "Security update for texlive",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for texlive fixes the following issue:\n\n- CVE-2018-17407: Prevent buffer overflow when handling of Type 1 fonts allowed\n  arbitrary code execution when a malicious font was loaded by one of the\n  vulnerable tools: pdflatex, pdftex, dvips, or luatex (bsc#1109673)\n",
			"title": "Details",
			"type": "General"
		},
