	"document_notes": [
		{
			"text": "Security update for xorg-x11-server",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for xorg-x11-server provides the following fix:\n\nSecurity issue fixed:\n\n- CVE-2018-14665: Local attackers could overwrite system files in any directory using the -logfile option and gain privileges (bsc#1111697)\n\nNon security issues fixed:\n\n- Do not write past the allocated buffer. (bsc#1078383)\n",
			"title": "Details",
			"type": "General"
		},
