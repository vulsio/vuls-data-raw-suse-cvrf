	"document_notes": [
		{
			"text": "Security update for net-snmp",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for net-snmp fixes the following issues:\n\n- CVE-2018-18065: _set_key in agent/helpers/table_container.c had a NULL Pointer Exception bug that can be used by an authenticated attacker to remotely cause the instance to crash via a crafted UDP packet, resulting in Denial of Service. (bsc#1111122)\n",
			"title": "Details",
			"type": "General"
		},
