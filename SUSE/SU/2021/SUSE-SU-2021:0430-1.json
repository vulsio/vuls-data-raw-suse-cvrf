	"document_notes": [
		{
			"text": "Security update for MozillaFirefox",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for MozillaFirefox fixes the following issues:\n\nFirefox Extended Support Release 78.7.1 ESR (bsc#1181848)\n\n- Fixed: Prevent access to NTFS special paths that could lead to filesystem corruption. \n- Buffer overflow in depth pitch calculations for compressed textures \n",
			"title": "Details",
			"type": "General"
		},
