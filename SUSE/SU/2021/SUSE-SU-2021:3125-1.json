	"document_notes": [
		{
			"text": "Security update for gtk-vnc",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for gtk-vnc fixes the following issues:\n\n- CVE-2017-5885: Correctly validate color map range indexes (bsc#1024268).\n- CVE-2017-5884: Fix bounds checking for RRE, hextile & copyrect encodings (bsc#1024266).\n- Fix crash when opening connection from a GSocketAddress (bsc#1046782).\n- Fix possible crash on connection failure (bsc#1188292).\n",
			"title": "Details",
			"type": "General"
		},
