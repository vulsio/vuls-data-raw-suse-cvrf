	"document_notes": [
		{
			"text": "Security update for dbus-1",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for dbus-1 fixes the following issues:\n\n- CVE-2020-35512: Fixed a bug where users with the same numeric UID could lead to use-after-free and undefined behaviour. (bsc#1187105)\n- CVE-2020-12049: Fixed a bug where a truncated messages lead to resource exhaustion. (bsc#1172505)\n",
			"title": "Details",
			"type": "General"
		},
