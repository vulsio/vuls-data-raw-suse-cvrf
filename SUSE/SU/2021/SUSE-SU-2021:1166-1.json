	"document_notes": [
		{
			"text": "Security update for wpa_supplicant",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for wpa_supplicant fixes the following issues:\n\n- CVE-2021-30004: Fixed an issue where forging attacks might have occured because AlgorithmIdentifier \n  parameters are mishandled in tls/pkcs1.c and tls/x509v3.c (bsc#1184348).\n",
			"title": "Details",
			"type": "General"
		},
