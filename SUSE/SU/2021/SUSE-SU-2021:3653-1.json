	"document_notes": [
		{
			"text": "Security update for qemu",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for qemu fixes the following issues:\n\nSecurity issues fixed:\n\n- Fix out-of-bounds write in UAS (USB Attached SCSI) device emulation (bsc#1189702, CVE-2021-3713)\n- Fix heap use-after-free in virtio_net_receive_rcu (bsc#1189938, CVE-2021-3748)\n",
			"title": "Details",
			"type": "General"
		},
