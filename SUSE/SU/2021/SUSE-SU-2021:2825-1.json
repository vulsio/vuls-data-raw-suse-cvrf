	"document_notes": [
		{
			"text": "Security update for openssl",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for openssl fixes the following security issue:\n\n- CVE-2021-3712: a bug in the code for printing certificate details could lead\n  to a buffer overrun that a malicious actor could exploit to crash the\n  application, causing a denial-of-service attack. [bsc#1189521]\n",
			"title": "Details",
			"type": "General"
