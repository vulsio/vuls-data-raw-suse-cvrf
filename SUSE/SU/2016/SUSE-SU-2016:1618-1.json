	"document_notes": [
		{
			"text": "Security update for mysql",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "\nThis update for mysql fixes the following issues:\n\n- bsc#959724: fix incorrect usage of sprintf/strcpy that caused possible buffer overflow issues at various places\n\nOn SUSE Linux Enterprise 11 SP4 this fix was not yet shipped:\n- Increase the key length (to 2048 bits) used in vio/viosslfactories.c for creating\n  Diffie-Hellman keys (Logjam Attack) [bnc#934789] [CVE-2015-4000]\n",
			"title": "Details",
			"type": "General"
		},
