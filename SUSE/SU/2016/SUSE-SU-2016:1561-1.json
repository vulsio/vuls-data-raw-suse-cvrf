	"document_notes": [
		{
			"text": "Security update for spice",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "spice was updated to fix two security issues.\n\nThese security issues were fixed:\n- CVE-2016-2150: Guest escape using crafted primary surface parameters (bsc#982386).\n- CVE-2016-0749: Heap-based buffer overflow in smartcard interaction (bsc#982385).\n  ",
			"title": "Details",
			"type": "General"
		},
