	"document_notes": [
		{
			"text": "Security update for cairo",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "\nThis update for cairo fixes the following issues:\n\n- CVE-2016-3190: Fixed an out-of-bound read in the fill_xrgb32_lerp_opaque_spans\n  function that might lead to a crash when processing a maliciously\n  crafted image file (bsc#971964).\n",
			"title": "Details",
			"type": "General"
		},
