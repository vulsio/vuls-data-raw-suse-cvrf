	"document_notes": [
		{
			"text": "Security update for LibVNCServer",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for LibVNCServer fixes the following issues:\n\nSecurity issues fixed:\n\n- CVE-2018-20749: Fixed a heap out of bounds write vulnerability in rfbserver.c (bsc#1123828)\n- CVE-2018-20750: Fixed a heap out of bounds write vulnerability in rfbserver.c (bsc#1123832)\n- CVE-2018-20748: Fixed multiple heap out-of-bound writes in VNC client code (bsc#1123823)\n",
			"title": "Details",
			"type": "General"
		},
