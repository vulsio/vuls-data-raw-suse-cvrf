	"document_notes": [
		{
			"text": "Security update for freeradius-server",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for freeradius-server fixes the following issues:\n\nSecurity issues fixed:\n\n- CVE-2019-11235: Fixed an authentication bypass related to the EAP-PWD Commit frame and insufficent validation of elliptic curve points (bsc#1132549).\n- CVE-2019-11234: Fixed an authentication bypass caused by reflecting privous values back to the server (bsc#1132664).\n",
			"title": "Details",
			"type": "General"
		},
