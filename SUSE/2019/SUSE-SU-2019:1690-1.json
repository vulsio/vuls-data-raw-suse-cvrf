	"document_notes": [
		{
			"text": "Security update for libvirt",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for libvirt fixes the following issue:\n\nSecurity issue fixed:\n\n- CVE-2019-10161: Fixed virDomainSaveImageGetXMLDesc API which could accept a path\n  parameter pointing anywhere on the system and potentially leading to execution\n  of a malicious file with root privileges by libvirtd (bsc#1138301).\n",
			"title": "Details",
			"type": "General"
		},
