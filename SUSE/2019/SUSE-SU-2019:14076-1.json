	"document_notes": [
		{
			"text": "Security update for gstreamer-0_10-plugins-base",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for gstreamer-0_10-plugins-base fixes the following issues:\n\nSecurity issues fixed:\n\n- CVE-2017-5837: Fixed a floating point exception in gst_riff_create_audio_caps (bsc#1024076).\n- CVE-2017-5844: Fixed a floating point exception in gst_riff_create_audio_caps (bsc#1024079).\n- CVE-2019-9928: Fixed a heap-based overflow in the rtsp connection parser (bsc#1133375).\n",
			"title": "Details",
			"type": "General"
		},
