	"document_notes": [
		{
			"text": "Security update for ffmpeg",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for ffmpeg fixes the following issues:\n\nSecurity issue fixed:\n\n- CVE-2018-14395: Fixed a divide-by-zero error in libavformat/movenc.c that allowed attackers to cause a DoS (bsc#1101889)\n- CVE-2018-14394: Fixed a divide-by-zero error in libavformat/movenc.c that allowed attackers to cause a DoS (bsc#1101888).\n",
			"title": "Details",
			"type": "General"
