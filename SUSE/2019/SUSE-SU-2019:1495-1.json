	"document_notes": [
		{
			"text": "Security update for MozillaThunderbird",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for MozillaThunderbird fixes the following security issues:\n\n- CVE-2019-11703: Fixed a heap-based buffer overflow in icalmemorystrdupanddequote() (bsc#1137595).\n- CVE-2019-11704: Fixed a heap-based buffer overflow in parser_get_next_char() (bsc#1137595).\n- CVE-2019-11705: Fixed a stack-based buffer overflow in icalrecur_add_bydayrules() (bsc#1137595).\n- CVE-2019-11706: Fixed a type confusion in icaltimezone_get_vtimezone_properties() (bsc#1137595).\n",
			"title": "Details",
			"type": "General"
		},
