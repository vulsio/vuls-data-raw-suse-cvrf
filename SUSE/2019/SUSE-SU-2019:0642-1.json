	"document_notes": [
		{
			"text": "Security update for lftp",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for lftp fixes the following issues:\n\nSecurity issue fixed: \n\n- CVE-2018-10916: Fixed an improper file name sanitization which could lead to loss of integrity of \n  the local system (bsc#1103367).\n  \nOther issue addressed:\n\n- The SSH login handling code detects password prompts more reliably (bsc#1120946).\n",
			"title": "Details",
			"type": "General"
		},
