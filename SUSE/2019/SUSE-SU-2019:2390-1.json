	"document_notes": [
		{
			"text": "Security update for openldap2",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for openldap2 fixes the following issues:\n\nSecurity issues fixed:\n\n- CVE-2019-13565: Fixed ssf memory reuse that leads to incorrect authorization of another connection, granting excess connection rights (ssf) (bsc#1143194).\n- CVE-2019-13057: Fixed rootDN of a backend that may proxyauth incorrectly to another backend, violating multi-tenant isolation (bsc#1143273).\n",
			"title": "Details",
			"type": "General"
		},
