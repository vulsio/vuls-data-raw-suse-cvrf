	"document_notes": [
		{
			"text": "Security update for docker-runc",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for docker-runc fixes the following issues:\n\t  \nSecurity issue fixed:\n\n- CVE-2019-5736: Effectively copying /proc/self/exe during re-exec to avoid\n  write attacks to the host runc binary, which could lead to a container\n  breakout (bsc#1121967)\n",
			"title": "Details",
			"type": "General"
		},
