	"document_notes": [
		{
			"text": "Security update for gnome-shell",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for gnome-shell fixes the following issues:\n\nSecurity issue fixed:\n\n- CVE-2019-3820: Fixed a partial lock screen bypass (bsc#1124493).\n\nFixed bugs:\n\n- Remove sessionList of endSessionDialog for security reasons (jsc#SLE-6660).\n",
			"title": "Details",
			"type": "General"
		},
