	"document_notes": [
		{
			"text": "Security update for qpdf",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for qpdf fixes the following issues:\n\n- CVE-2022-34503: Fixed a heap buffer overflow via the function QPDF::processXRefStream (bsc#1201830).\n- CVE-2021-36978: Fixed heap-based buffer overflow in Pl_ASCII85Decoder::write (bsc#1188514).\n",
			"title": "Details",
			"type": "General"
		},
