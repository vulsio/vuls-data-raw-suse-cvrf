	"document_notes": [
		{
			"text": "Security update for ImageMagick",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for ImageMagick fixes the following issues:\n\n  - CVE-2022-32545: Fixed an outside the range of representable values of type. (bsc#1200388)\n  - CVE-2022-32546: Fixed an outside the range of representable values of type. (bsc#1200389)\n  - CVE-2022-32547: Fixed a load of misaligned address at MagickCore/property.c. (bsc#1200387)\n",
			"title": "Details",
			"type": "General"
		},
