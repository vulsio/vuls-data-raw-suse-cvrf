	"document_notes": [
		{
			"text": "Security update for python3",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for python3 fixes the following issues:\n\n- CVE-2022-37454: Fixed a buffer overflow in hashlib.sha3_* implementations. (bsc#1204577)\n- CVE-2020-10735: Fixed a bug to limit amount of digits converting text to int and vice vera. (bsc#1203125)\n",
			"title": "Details",
			"type": "General"
		},
