	"document_notes": [
		{
			"text": "Security update for libnl-1_1",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for libnl-1_1 fixes the following issues:\n\n- CVE-2017-0386: Fixed an issue that could enable a local malicious\n  application to execute arbitrary code within the context of a\n  different process. This only affects setups were libnl is passed\n  untrusted arguments. (bsc#1020123)\n",
			"title": "Details",
			"type": "General"
		},
