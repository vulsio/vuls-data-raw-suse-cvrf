	"document_notes": [
		{
			"text": "Security update for libexif",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for libexif fixes the following issues:\n\n- CVE-2020-0181: Fixed an integer overflow that could lead to denial of service\n  (bsc#1172802).\n- CVE-2020-0198: Fixed and unsigned integer overflow that could lead to denial\n  of service (bsc#1172768).\n- CVE-2020-0452: Fixed a buffer overflow check that could be optimized away\n  by the compiler (bsc#1178479).\n",
			"title": "Details",
			"type": "General"
		},
