	"document_notes": [
		{
			"text": "Security update for ImageMagick",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for ImageMagick fixes the following issues:\n\n- CVE-2022-2719: Fixed a reachable assertion that could lead to denial\n  of service via a crafted file (bsc#1202250).\n- CVE-2022-28463: Fixed a buffer overflow that could be triggered by\n  a crafted input file (bsc#1199350).\n",
			"title": "Details",
			"type": "General"
