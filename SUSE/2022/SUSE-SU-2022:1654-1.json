	"document_notes": [
		{
			"text": "Security update for documentation-suse-openstack-cloud, kibana, openstack-keystone, openstack-monasca-notification",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for documentation-suse-openstack-cloud, kibana, openstack-keystone, openstack-monasca-notification fixes the following issues:\n\n- CVE-2021-22141: Fixed URL redirection flaw (bsc#1186868).\n- CVE-2021-38155: Fixed information disclosure during account locking (bsc#1189390).\n\nThe following non-security bugs were fixed:\n\n- Fix smtp server authentication (bsc#1197204)\n",
			"title": "Details",
			"type": "General"
		},
