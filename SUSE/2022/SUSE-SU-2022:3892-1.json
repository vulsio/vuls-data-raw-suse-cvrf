	"document_notes": [
		{
			"text": "Security update for exiv2",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for exiv2 fixes the following issues:\n\n  - CVE-2019-13111: Fixed nteger overflow in WebPImage:decodeChunks (bsc#1142679).\n  - CVE-2021-29463: Fixed out-of-bounds read (bsc#1185913).\n  - CVE-2021-34334: Fixed a DoS due to integer overflow in loop counter bug (bsc#1189338).\n",
			"title": "Details",
			"type": "General"
		},
