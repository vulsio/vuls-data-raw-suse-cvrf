	"vulnerability": [
		{
			"notes": [
				{
					"text": "cifs-utils through 6.14, with verbose logging, can cause an information leak when a file contains = (equal sign) characters but is not a valid credentials file.",
					"title": "Vulnerability Description",
					"type": "General"
				}
