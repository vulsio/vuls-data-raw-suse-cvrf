	"document_notes": [
		{
			"text": "Security update for u-boot",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for u-boot fixes the following issues:\n\n- CVE-2022-30790: Fixed an arbitrary out-of-bounds write in the IP defragmentation (bsc#1200364).\n- CVE-2022-30552: Fixed an out-of-bounds write in the IP defragmentation (bsc#1200363).\n- CVE-2022-34835: Fixed stack buffer overflow vulnerability in i2c md command (bsc#1201214).\n",
			"title": "Details",
			"type": "General"
		},
