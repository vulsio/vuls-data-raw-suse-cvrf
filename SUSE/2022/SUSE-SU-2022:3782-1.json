	"document_notes": [
		{
			"text": "Security update for libmad",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for libmad fixes the following issues:\n\n  - CVE-2017-8373: Fixed heap-based buffer overflow in mad_layer_III (bsc#1036968).\n  - CVE-2017-8372: Fixed assertion failure in layer3.c (bsc#1036969).\n\n",
			"title": "Details",
			"type": "General"
