	"document_notes": [
		{
			"text": "Security update for cairo",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for cairo fixes the following issues:\n\n- CVE-2017-7475: Fixed a segfault in get_bitmap_surface due to malformed font\n  (bsc#1036789).\n- CVE-2016-9082: fix a segfault when using >4GB images since int values were used\n  for pointer operations (bsc#1007255).\n\n",
			"title": "Details",
			"type": "General"
		},
