	"document_notes": [
		{
			"text": "Security update for cloud-init",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for cloud-init fixes the following security issues:\n\n- CVE-2020-8631: Replaced the theoretically predictable deterministic RNG with the system RNG (bsc#1162937).\n- CVE-2020-8632: Increased the default random password length from 9 to 20 (bsc#1162936).\n",
			"title": "Details",
			"type": "General"
		},
