	"document_notes": [
		{
			"text": "Security update for dnsmasq",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for dnsmasq fixes the following issues:\n\nSecurity issue fixed:\n\n- CVE-2019-14834: Fixed a memory leak which could have allowed to remote attackers \n  to cause denial of service via DHCP response creation (bsc#1154849)\n\nOther issue addressed:  \n\n- Removed cache size limit (bsc#1138743).\n\n",
			"title": "Details",
			"type": "General"
		},
