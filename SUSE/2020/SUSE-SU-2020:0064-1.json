	"document_notes": [
		{
			"text": "Security update for openssl-1_0_0",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for openssl-1_0_0 fixes the following issues:\n\nSecurity issue fixed:\n\n- CVE-2019-1551: Fixed an overflow bug in the x64_64 Montgomery squaring procedure used in exponentiation with 512-bit moduli (bsc#1158809).\n",
			"title": "Details",
			"type": "General"
		},
