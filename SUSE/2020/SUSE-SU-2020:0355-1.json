	"document_notes": [
		{
			"text": "Security update for pcp",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for pcp fixes the following issues:\n\nSecurity issue fixed:\n\n- CVE-2019-3695: Fixed a local privilege escalation of the pcp user during package update (bsc#1152763).\n\nNon-security issue fixed:\n\n- Fixed an dependency issue with pcp2csv (bsc#1129991).\n",
			"title": "Details",
			"type": "General"
		},
