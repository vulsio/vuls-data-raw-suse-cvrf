	"document_notes": [
		{
			"text": "Security update for dovecot22",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for dovecot22 fixes the following issues:\n\n- CVE-2020-12673: improper implementation of NTLM does not check message buffer size (bsc#1174922).\n- CVE-2020-12674: improper implementation of RPA mechanism (bsc#1174923).\n",
			"title": "Details",
			"type": "General"
