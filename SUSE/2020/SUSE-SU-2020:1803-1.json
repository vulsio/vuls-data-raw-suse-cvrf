	"document_notes": [
		{
			"text": "Security update for squid",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for squid fixes the following issues:\n\n- CVE-2020-14059: Fixed an issue where a client could potentially deny the service of a server \n  during TLS Handshake (bsc#1173304).\n- CVE-2019-18860: Fixed handling of invalid domain names in cachemgr.cgi (bsc#1167373).\n",
			"title": "Details",
			"type": "General"
		},
