	"document_notes": [
		{
			"text": "Security update for xorg-x11-server",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for xorg-x11-server fixes the following issues:\n\n- CVE-2020-25712: Fixed a heap-based buffer overflow which could have led to privilege escalation (bsc#1177596).\n- CVE-2020-14360: Fixed an out of bounds memory accesses on too short request which could lead to denial of service (bsc#1174908).\n",
			"title": "Details",
			"type": "General"
