	"document_notes": [
		{
			"text": "Security update for udisks2",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for udisks2 fixes the following issues:\n\nFollowing security issues was fixed:\n\n- CVE-2018-17336: A format string vulnerability in udisks_log (bsc#1109406)\n\nFollowing non-security issues were fixed:\n\n- strip trailing newline from sysfs raid level information (bsc#1091274)\n- Fix watcher error for non-redundant raid devices. (bsc#1091274)\n",
			"title": "Details",
			"type": "General"
		},
