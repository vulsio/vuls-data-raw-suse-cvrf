	"vulnerability": [
		{
			"notes": [
				{
					"text": "Multiple integer signedness errors in the (1) process_bin_sasl_auth, (2) process_bin_complete_sasl_auth, (3) process_bin_update, and (4) process_bin_append_prepend functions in Memcached 1.4.5 and earlier allow remote attackers to cause a denial of service (crash) via a large body length value in a packet.",
					"title": "Vulnerability Description",
					"type": "General"
				}
			],
			"cve": "CVE-2011-4971",
