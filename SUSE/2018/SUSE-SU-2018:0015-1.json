	"document_notes": [
		{
			"text": "Security update for libvorbis",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for libvorbis fixes the following issues:\n\n- CVE-2017-14633: out-of-bounds array read vulnerability exists in\n  function mapping0_forward() could lead to remote denial of service (bsc#1059811)\n- CVE-2017-14632: Remote Code Execution upon freeing uninitialized\n  memory in function vorbis_analysis_headerout(bsc#1059809)\n",
			"title": "Details",
			"type": "General"
		},
