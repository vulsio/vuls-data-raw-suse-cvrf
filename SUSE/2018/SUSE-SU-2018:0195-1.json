	"document_notes": [
		{
			"text": "Security update for libexif",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for libexif fixes the following security issue:\n\n- CVE-2017-7544: Fixed out-of-bounds heap read vulnerability in\n  exif_data_save_data_entry function in libexif/exif-data.c caused by improper\n  length computation of the allocated data of an ExifMnote entry which can cause\n  denial-of-service or possibly information disclosure (bsc#1059893)\n",
			"title": "Details",
			"type": "General"
		},
