	"document_notes": [
		{
			"text": "Recommended update for freetype2",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "\nThis update of freetype2 fixes a regression introduced by the security\nfix for CVE-2014-9671.\n\nThis is not itself a security issue, it just improves on a previous one.\n\nThis update is needed for LSB 5 fontconfig usage.\n",
			"title": "Details",
			"type": "General"
