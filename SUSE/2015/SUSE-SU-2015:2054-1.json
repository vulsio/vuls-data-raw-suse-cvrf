	"vulnerability": [
		{
			"notes": [
				{
					"text": "driver/subprocs.c in XScreenSaver before 5.34 does not properly perform an internal consistency check, which allows physically proximate attackers to bypass the lock screen by hot swapping monitors.",
					"title": "Vulnerability Description",
					"type": "General"
				}
			],
