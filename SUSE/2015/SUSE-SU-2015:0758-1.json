	"vulnerability": [
		{
			"notes": [
				{
					"text": "Buffer overflow in copy.c in Mutt before 1.5.23 allows remote attackers to cause a denial of service (crash) via a crafted RFC2047 header line, related to address expansion.",
					"title": "Vulnerability Description",
					"type": "General"
				}
			],
			"cve": "CVE-2014-0467",
