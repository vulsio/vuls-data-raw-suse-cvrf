	"document_notes": [
		{
			"text": "Recommended update for openldap2",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "openldap2 was updated to fix one security issue.\n\nThis security issue was fixed:\n- CVE-2015-4000: The Logjam Attack / weakdh.org (bsc#937766).\n\nThis non-security issue was fixed:\n- bsc#932773: ldapmodify failed with DOS format LDIF files containing '-' separator.\n  ",
			"title": "Details",
			"type": "General"
		},
