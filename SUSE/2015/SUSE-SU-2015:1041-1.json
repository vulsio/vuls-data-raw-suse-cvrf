	"document_notes": [
		{
			"text": "Security update for cups",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "The following issues are fixed by this update:\n\n* CVE-2012-5519: privilege escalation via cross-site scripting\n  and bad print job submission used to replace cupsd.conf on server (bsc#924208).\n* CVE-2015-1158: Improper Update of Reference Count\n* CVE-2015-1159: Cross-Site Scripting\n\n",
			"title": "Details",
			"type": "General"
		},
