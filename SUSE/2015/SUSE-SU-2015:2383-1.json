	"document_notes": [
		{
			"text": "Security update for xfsprogs",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "xfsprogs was updated to fix one security vulnerability and several bugs.\n\n- Handle unwanted data disclosure in xfs_metadump (bsc#939367, CVE-2012-2150)\n- Fix segfault during xfs_repair run (bsc#911866)\n- Fix definition of leaf attribute block to avoid gcc optimization xfsprogs-fix-leaf-block-definition\n",
			"title": "Details",
			"type": "General"
		},
