	"document_notes": [
		{
			"text": "Security update for libvirt",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "\nThis update for libvirt fixes the following issues:\n\nSecurity issues fixed:\n- CVE-2016-5008: empty VNC password disables authentication (bsc#987527)\n\nBugs fixed:\n- bsc#970906: Fixed a race condition in xenstore event handling.\n- bsc#952889: Change hap setting to align with Xen behavior.\n- Fixed 'make check' failures.\n  ",
			"title": "Details",
			"type": "General"
		},
