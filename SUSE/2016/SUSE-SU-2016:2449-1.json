	"document_notes": [
		{
			"text": "Security update for curl",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "\nThis update for curl fixes the following issues:\n\n- CVE-2016-5419: TLS session resumption client cert bypass (bsc#991389)\n- CVE-2016-5420: Re-using connections with wrong client cert (bsc#991390)\n- CVE-2016-7141: Fixed incorrect reuse of client certificates (bsc#997420).\n",
			"title": "Details",
			"type": "General"
		},
