	"vulnerability": [
		{
			"notes": [
				{
					"text": "eCryptfs 104 and earlier uses a default salt to encrypt the mount passphrase, which makes it easier for attackers to obtain user passwords via a brute force attack.",
					"title": "Vulnerability Description",
					"type": "General"
				}
			],
