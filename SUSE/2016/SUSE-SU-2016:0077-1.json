	"document_notes": [
		{
			"text": "Security update for gnutls",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "\nThis update for gnutls fixes the following security issues:\n\n- CVE-2015-8313: First byte of the padding in CBC mode is not checked (bsc#957568)\n- CVE-2015-2806: Two-byte stack overflow in asn1_der_decoding (bsc#924828)\n",
			"title": "Details",
			"type": "General"
		},
