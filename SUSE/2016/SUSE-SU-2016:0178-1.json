	"document_notes": [
		{
			"text": "Security update for libxml2",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "\nThis update for libxml2 fixes the following security issue:\n\n- CVE-2015-8710: Parsing short unclosed HTML comment could cause uninitialized memory access, which allowed remote attackers to read contents from previous HTTP requests depending on the application (bsc#960674)\n",
			"title": "Details",
			"type": "General"
