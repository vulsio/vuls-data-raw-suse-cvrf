	"document_notes": [
		{
			"text": "Security update for libimobiledevice, usbmuxd",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "libimobiledevice, usbmuxd were updated to fix one security issue.\n\nThis security issue was fixed:\n- CVE-2016-5104: Sockets listening on INADDR_ANY instead of only locally (982014).\n",
			"title": "Details",
			"type": "General"
