	"document_notes": [
		{
			"text": "Security update for java-1_7_0-openjdk",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "This update for java-1_7_0-openjdk to version 2.6.6 fixes five security issues.\n\nThese security issues were fixed:\n- CVE-2016-0686: Ensure thread consistency (bsc#976340).\n- CVE-2016-0687: Better byte behavior (bsc#976340).\n- CVE-2016-0695: Make DSA more fair (bsc#976340).\n- CVE-2016-3425: Better buffering of XML strings (bsc#976340).\n- CVE-2016-3427: Improve JMX connections (bsc#976340).\n",
			"title": "Details",
			"type": "General"
		},
